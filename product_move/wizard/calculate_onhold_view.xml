<?xml version="1.0"?>
<odoo>
        <record id="product_move_calculate_onhold_form" model="ir.ui.view">
            <field name="name">product_move.calculate_onhold.wizard.form</field>
            <field name="model">product_move.calculate_onhold.wizard</field>
            <field name="arch" type="xml">
                <form string="Calculate Onhold Quantity">
                    <group>
                        <button name="calculate_onhold" string="Calculate" type="object" class="btn-primary"/>
                    </group>
                    <footer>
                        <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="product_move_calculate_onhold_action" model="ir.actions.act_window">
            <field name="name">Calculate Onhold Quantity</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">product_move.calculate_onhold.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="product_move_calculate_onhold_form"/>
            <field name="target">new</field>
        </record>
</odoo>